function createInstructor(firstName, lastName){
    return {
      firstName: firstName,
      lastName: lastName
    }
  }

  //“Write an ES2015 Version”.

  function createInstructorRefactor(firstName, lastName){
    return {
        firstName,
        lastName
    }
  }

